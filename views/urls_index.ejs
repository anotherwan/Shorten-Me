<!DOCTYPE html>
  <html>
  <head>
    <% include ./partials/_materializeHead %>
  </head>
  <body>
    <% include ./partials/_materializeBody %>
    <% include ./partials/_header %>
    <br>
    <br>
    <div class="container">
      <div class="row">
        <a class="btn-floating btn-large waves-effect waves-light blue-grey" href="/urls/new"><i class="material-icons">add</i></a>
        <br>
        <% for (var key in urls) { %>
          <label> Short Link: </label>
          <br>
           <a href="/urls/<%= key %>"> <%= key %> </a>
          <br>
          <label> Long Link: </label>
          <br>
          <%= urls[key].longURL %>
          <br>
          <form method="POST" action="/urls/<%= key %>" enctype="application/x-www-form-urlencoded">
            <input type="hidden" name="_method" value="DELETE">
              <button type="submit">Delete</button>
          </form>
          <form method="POST" action="/urls/<%= key %>/edit" enctype="application/x-www-form-urlencoded">
            <input type="hidden" name="_method" value="GET">
              <button type="submit">Edit</button>
          </form>
        <% } %>
      </div>
    </div>
  </body>
  </html>
